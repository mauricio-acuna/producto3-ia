{
  "alive-status-codes": [200, 206],
  "check-modified-files-only": false,
  "base-url": "",
  "file-extension": ".md",
  "ignore-patterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    }
  ],
  "ignore-status-codes": [0, 500, 502, 503],
  "timeout": "20s",
  "retry-on-429": true,
  "retry-count": 3,
  "fallback-http-status-codes": [400, 401, 403, 404, 500, 502, 503],
  "verbose": false
}
